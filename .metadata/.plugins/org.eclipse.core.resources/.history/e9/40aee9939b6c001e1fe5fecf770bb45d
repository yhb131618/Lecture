package chap04_controlstatement;

public class _07_DuplicatedFor {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 1. 2 ~ 9단 까지 구구단 출력
		for(int i = 2; i <= 9; i++) {
			for(int j = 1; j <= 9; j++) {
				//72번 실행
				System.out.print(i + " * " + j + " = " 
										+ (i * j) + "   ");
			}
			// 8번실행(2~9)
			System.out.println();
		}
		System.out.println("--------------------------");
		
		// 2. 
		// *****
		// *****
		// *****
		// *****
		// *****
	}

}
